{% extends '::base.html.twig' %}


{% block pageheader %}
    <h1>New User</h1>
{% endblock %}

{% block body -%}

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <div class="form-group">
        <div class="col-sm-2"></div>
        <div class="col-sm-10">
            <input type="submit" value="Create" class="btn btn-primary" />
            <a href="{{ path('user') }}" class="btn btn-warning">Cancel</a>
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}
